}

.contactWay text:nth-child(1) {
  font-family: PingFang-SC-Medium;
  font-size: 29rpx;
}

.contactWay text:nth-child(2) {
  font-family: PingFang-SC-Regular;
  font-size: 29rpx;
}

.contactWay:after {

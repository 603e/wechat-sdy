.address {
  width: 700rpx;
  padding: 17rpx 0 19rpx 39rpx;
  height: 68rpx;
  line-height: 68rpx;
}

.address text:nth-child(1) {
  font-family: PingFang-SC-Medium;
  font-size: 29rpx;
}

.address text:nth-child(2) {
  font-family: PingFang-SC-Regular;
  font-size: 29rpx;
}

.address:after {
